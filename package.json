{
  "name": "girlscodemk-libraryapp-js",
  "version": "1.0.0",
  "main": "dist",
  "scripts": {
    "dev": "npx nodemon -w src --exec \"babel-node src\"",
    "db:reset": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate",
    "clean": "rm -rf build",
    "build-babel": "mkdir -p build && babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "node build/index.js",
    "postinstall": "npm run build"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "repository": "git@github.com:GirlsCodeMK/libraryapp-hackathon-js-starthere",
  "author": "GirlsCode MK",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "connect-ensure-login": "^0.1.1",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-ejs-layouts": "^2.5.0",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
